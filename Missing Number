Problem Link :- https://cses.fi/problemset/task/1083/

#include<bits/stdc++.h> 
using namespace std ;

/*
    Intuition :- Ekhane 1 theke n porjonto shobgula number thakar
    kotha . But ekta number missing ache . 

    Ei problem tar solution holo 1 theke n projonto number gular je
    jugfol houwar kotha tar theke array tar summation minus kore
    dile jei value pawa jabe oi value ta e missing ache .
*/

int main(){
    long long n ; cin >> n ; vector<long long> vec ;
    for(int i = 1 ; i <= n-1 ; i++){
        long long x ; cin >> x ; vec.push_back(x) ;
    }
    long long sum = accumulate(vec.begin(),vec.end(),0LL) ;
    long long sumOfFirstN_NaturalNumbers = (n * (n+1)) / 2 ;
    cout << sumOfFirstN_NaturalNumbers - sum << '\n' ;
}

