Problem Link :- https://cses.fi/problemset/task/1070/

#include<bits/stdc++.h> 
using namespace std ;

/*
    Intuition :- 1 size er permutation toiri kora shomvob karon ar 
    kuno element nai . 2 ar 3 size er permutation toiri kora shomvob
    na . 3 theke boro jekuno size er permuation toiri kora shomvob .
    Niche 4 theke n porjonto shobgula value er jonne general logic 
    likha hoise .
*/

int main(){
    int n ; cin >> n ;
    if(n==1) cout << 1 << '\n' ;
    else if(n<4) cout << "NO SOLUTION" << '\n' ; 
    else{
        if((n&1)){
            for(int i = n ; i >= 1 ; i-=2 ) cout << i << " " ;
            for(int i = n-1 ; i > 0 ; i -= 2) cout << i << " " ;
            cout << '\n' ;
        }else{
            for(int i = n-1 ; i > 0 ; i -= 2) cout << i << " " ;
            for(int i = n ; i >= 1 ; i-=2 ) cout << i << " " ;
            cout << '\n' ;
        }
    }
}

